/var/www/MISP/app/tmp/logs/mispsyslog.log
/var/www/MISP/app/tmp/logs/misp-workers.log 
/var/www/MISP/app/tmp/logs/misp-workers-errors.log
{
    su root www-data
    rotate 5
    dateext
    missingok
    notifempty
    compress
    weekly
    size 50M
    maxsize 500M
    copytruncate
}

